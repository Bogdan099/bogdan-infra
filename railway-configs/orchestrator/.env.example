# Orchestrator Service Environment Variables

# Application Settings
APP_NAME=orchestrator
DEBUG=false
LOG_LEVEL=info

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/orchestrator_db

# API Settings
API_HOST=0.0.0.0
API_PORT=8000

# Service URLs
QDRANT_URL=http://qdrant-integration:8001
NOTION_SYNC_URL=http://notion-sync:8002
SCHEDULER_URL=http://scheduler:8003
NOTIFICATIONS_URL=http://notifications:8004

# Authentication
JWT_SECRET=your-super-secret-jwt-key
API_KEY=your-api-key

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Monitoring
SENTRY_DSN=your-sentry-dsn